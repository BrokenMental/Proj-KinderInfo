<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="us.inhatc.mappers.KinderMapper">
	<select id="selectgrid" resultType="us.inhatc.domain.GridVO">
		SELECT *
		FROM gridd
	</select>
	<select id="selectchartsolo" resultType="us.inhatc.domain.ChartVO">
		SELECT solo
		FROM chartd
	</select>
	<select id="selectcharttoget" resultType="us.inhatc.domain.ChartVO">
		SELECT toget
		FROM chartd
	</select>
	<select id="selectchartairtel" resultType="us.inhatc.domain.ChartVO">
		SELECT airtel
		FROM chartd
	</select>
	<select id="selectchartetc" resultType="us.inhatc.domain.ChartVO">
		SELECT etc
		FROM chartd
	</select>
	
	<select id="selectSidoName" resultType="us.inhatc.domain.SidoVO">
		SELECT sidoName
		FROM sido_code
	</select>
	<select id="selectSigunguName" resultType="us.inhatc.domain.SidoVO">
		select sigunguName
		from sigungu_code
		where sigunguCode like concat((select sidoCode from sido_code where sidoName = #{sidoName}),'%');
	</select>
</mapper>